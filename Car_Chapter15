package kadai_015;

public class Car_Chapter15 {
	private int gear = 1;
	private int speed =10;
}

public void gearChange(int afterGear) {
	if (afterGear > 0 && afterGear < 6) {
		this.speed = afterGear * 10;
	} else {
		this.speed = 10;
	}
}

public void run() {
	System.out.println("速度は時速" + rhis.speed + "kmです");
}